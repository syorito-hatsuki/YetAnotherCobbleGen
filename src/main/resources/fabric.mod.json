{
  "schemaVersion": 1,
  "id": "yacg",
  "version": "${version}",
  "name": "Yet Another Cobblestone Generator",
  "description": "Flexible customizable generators",
  "authors": [
    "Syorito Hatsuki",
    "Hasti",
    "Denfop"
  ],
  "contact": {
    "sources": "https://github.com/syorito-hatsuki/yet-another-cobble-gen",
    "issues": "https://github.com/syorito-hatsuki/yet-another-cobble-gen/issues"
  },
  "license": "MIT Licence",
  "icon": "assets/yacg/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.yacg.YetAnotherCobblestoneGenerator"
      }
    ],
    "emi": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.yacg.integration.emi.YacgEmiPlugin"
      }
    ],
    "jade": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.yacg.integration.jade.YacgJadePlugin"
      }
    ],
    "rei_client": [
      {
        "adapter": "kotlin",
        "value": "dev.syoritohatsuki.yacg.integration.rei.YacgReiPlugin"
      }
    ]
  },
  "custom": {
    "duckyupdater": {
      "modrinthId": "xPsKRMUF"
    },
    "fstats": 32,
    "mcb": [
      {
        "name": "Ducky House",
        "fillColor": -13730510,
        "outlineColor": -11751600
      }
    ]
  },
  "mixins": [
    "yacg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "fabric": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": ">=1.21",
    "java": ">=${javaVersion}"
  },
  "recommends": {
    "modmenu-badges-lib": "*",
    "emi": "*",
    "jade": "*",
    "rei": "*"
  },
  "suggests": {
    "fstats-api": "*"
  }
}
